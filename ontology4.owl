<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/ontology4#"
     xml:base="http://example.org/ontology4"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
    <owl:Ontology rdf:about="http://example.org/ontology4"/>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/ontology4#Person"/>
    <owl:Class rdf:about="http://example.org/ontology4#Employee"/>
    <owl:Class rdf:about="http://example.org/ontology4#Organization"/>

    <!-- Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/ontology4#employedBy"/>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology4#name"/>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology4#age"/>

</rdf:RDF>
